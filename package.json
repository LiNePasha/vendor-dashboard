{
  "name": "vendor-dashboard",
  "version": "0.1.0",
  "private": true,
  "main": "electron/main.js",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "electron": "electron .",
    "electron-dev": "concurrently \"npm run dev\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-build": "next build && electron-builder",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "com.spare2app.vendor-pos",
    "productName": "Vendor POS",
    "directories": {
      "output": "dist",
      "buildResources": "public"
    },
    "files": [
      ".next/**/*",
      "app/**/*",
      "components/**/*",
      "public/**/*",
      "electron/**/*",
      "package.json",
      "next.config.mjs"
    ],
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "public/icons/icon-512x512.png"
    }
  },
  "dependencies": {
    "axios": "^1.12.2",
    "cloudinary": "^2.7.0",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "formidable": "^3.5.4",
    "googleapis": "^166.0.0",
    "groq-sdk": "^0.37.0",
    "localforage": "^1.10.0",
    "multer": "^2.0.2",
    "next": "15.5.7",
    "next-pwa": "^5.6.0",
    "papaparse": "^5.5.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "recharts": "^3.7.0",
    "xlsx": "^0.18.5",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "tailwindcss": "^4"
  }
}
